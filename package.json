{
  "name": "react_template_repo",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "test": "mocha server/tests/**/*.test.js",
    "install-all": "concurrently \"npm install\" \"cd client && npm install\"",
    "client": "cd client && npm start",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"nodemon server/server.js\" \"cd client && npm run start\"",
    "build": "cd client && npm build",
    "start": "NODE_ENV=production node server/server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "ci-client": "cd client && npm ci",
    "lint": "eslint --ext .js --fix server/**/*.js",
    "lint-client": "cd client && npm run lint && cd ..",
    "test-ci": "mocha server/tests/**/*.test.js",
    "build-client": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UFWebApps/React_Template_Repo.git"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-underscore-dangle": "off",
      "no-restricted-syntax": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "keywords": [],
  "author": "Dakota Rennemann",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "config": "^3.2.4",
    "connect-mongo": "^3.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "handlebars": "^4.5.3",
    "js-file-download": "^0.4.8",
    "markdown-pdf": "^10.0.0",
    "material-design-icons": "^3.0.1",
    "memory-streams": "^0.1.3",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "print-js": "^1.0.61",
    "react-icons": "^3.8.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "validator": "^12.0.0"
  },
  "homepage": "https://github.com/UFWebApps/React_Template_Repo#readme",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "sinon": "^7.5.0"
  }
}
